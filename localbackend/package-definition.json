{
  "name": "stonerstats-localbackend",
  "version": "1.0.0",
  "description": "Local backend server for StonerStats app with JSON-based storage",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build-exe": "pkg . --output stonerstats-server.exe"
  },
  "keywords": ["stonerstats", "backend", "express", "api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": ["data/**/*"],
    "targets": ["node18-win-x64"],
    "outputPath": "dist"
  }
}
